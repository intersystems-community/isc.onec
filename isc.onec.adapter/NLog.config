<?xml version="1.0" encoding="utf-8" ?>
<nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
 
    <targets>
        <target name="logfile" xsi:type="File" fileName="c:\tmp\isc.onec.adapter.log" layout="${longdate} ${threadid} ${message} ${exception:format=tostring}"/>
        <target name="eventlog" xsi:type="EventLog" log="Application" source="isc.onec.adapter" layout="${longdate} ${threadid} ${message} ${exception:format=tostring}"/>
    </targets>

    <rules>
        <logger name="*" minlevel="Debug" writeTo="logfile" />
      <!--logger name="*" minlevel="Info" writeTo="eventlog" /-->
    </rules>
</nlog>